{
  "extends": ["tslint-react"],
  "rules": {
    "max-line-length": {
      "options": [
        88
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        88
      ]
    }
  }
}

